public class EscenaExterna extends Escena {
    private String tipo; // DÃ­a o Noche
    private Locacion locacion;

    public EscenaExterna(String codigo, String descripcion, String tipo, Locacion locacion) {
        super(codigo, descripcion);
        this.tipo = tipo;
        this.locacion = locacion;
    }

    @Override
    public String toString() {
        return "EscenaExterna{" +
                "tipo='" + tipo + '\'' +
                ", locacion=" + locacion +
                ", " + super.toString() +
                '}';
    }
}
